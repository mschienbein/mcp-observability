{
  "commands": [
    "set -euxo pipefail",
    "echo ==== docker active ====",
    "systemctl is-active docker || true",
    "echo ==== docker ps ====",
    "docker ps || true",
    "echo ==== http ping ====",
    "curl -fsS http://127.0.0.1:8123/ping || echo FAIL",
    "echo ==== listeners 8123 ====",
    "ss -ltnp | grep 8123 || true",
    "echo ==== listeners 9000 ====",
    "ss -ltnp | grep 9000 || true",
    "echo ==== clickhouse logs tail ====",
    "docker logs --tail=50 clickhouse || true"
  ]
}
